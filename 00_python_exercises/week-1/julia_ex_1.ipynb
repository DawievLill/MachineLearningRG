   "source": [
    "function a_plus_abs_b(a, b)\n",
    "\n",
    "    if b < 0\n",
    "        f = (a, b) -> a + -b\n",
    "    else\n",
    "        f = (a, b) -> a + b\n",
    "    return f(a, b)\n",
    "\n",
    "    end\n",
    "end"
   ]
